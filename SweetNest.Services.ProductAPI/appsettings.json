{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "ConnectionStrings": {
        "DefaultConnection": "YOUR_CONNECTIONSTRING"
    },
    "ApiSettings": {
        "Secret": "YourSecret",
        "Issuer": "YourIssuer",
        "Audience": "YourAudience"
    },
    "GcpSettings": {
        "CloudStorage": {
            "BucketName": "YOUR_BUCKETNAME",
            "FilePath": "YOUR_FILEPATH"
        }
    }
}
